{
  "version": "0.1.6",
  "license": "MIT",
  "description": "Theminator replaces the colors and dimensions (e.g. #123456 or 12rem) in your theme with objects containing useful methods that you can use to create derived values, like a darkened version of the background color or a multiplied border width for a hover effect.",
  "repository": "AndrewPrifer/theminator",
  "keywords": [
    "react",
    "theme",
    "theming",
    "styled-components",
    "emotion",
    "jss",
    "cssinjs",
    "css-in-js",
    "chroma"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "theminator",
  "author": "Andrew Prifer",
  "module": "dist/theminator.esm.js",
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "husky": "^4.2.1",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@types/chroma-js": "^2.0.0",
    "chroma-js": "^2.1.0",
    "deep-map-object": "^0.0.5",
    "postcss-value-parser": "^4.0.2"
  }
}
